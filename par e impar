#include <stdio.h>
int main (){

    int x;
    
    scanf("%d", &x);

    if (x >= 0) {
         if (x % 2 == 0){
             printf("Positivo par\n");
            }
         else {
             printf("Negativo impar\n");
            }
    }
    else {
        if (x % 2 == 0) {
            printf("Positivo par\n");
        }
        else {
            printf("Negativo impar\n");
        }
    }
return 0;
}
